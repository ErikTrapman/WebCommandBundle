{
    "name": "eriktrapman/webcommand-bundle",
    "description": "Enables you to start console commands from a browser when there's no SSH-access",
    "keywords": ["symfony", "console", "command", "controller", "web"],
    "homepage": "https://github.com/ErikTrapman/WebCommandBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Erik Trapman"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/form": ">=2.1.0",
        "symfony/http-kernel": ">=2.1.0",
        "symfony/console": ">=2.1.0",
        "symfony/finder": ">=2.1.0",
        "symfony/event-dispatcher": ">=2.1.0"
    },
    "require-dev": {
        "symfony/symfony": ">=2.1.0"
    },
    "autoload": {
        "psr-0": { "ErikTrapman\\Bundle\\WebCommandBundle": "" }
    },
    "target-dir": "ErikTrapman/Bundle/WebCommandBundle"
}
